RewriteEngine On
php_value session.gc_maxlifetime 4200
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !^/pertama_pa/temp/
RewriteCond %{REQUEST_URI} !^/pertama_pa/public/
RewriteCond %{REQUEST_URI} !^/pertama_pa/plugin/
RewriteRule ^(.*)$ public/index.php?url=$1 [NC,QSA,L]